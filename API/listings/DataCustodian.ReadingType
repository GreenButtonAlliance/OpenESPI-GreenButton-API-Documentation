			[
        {
          "method": "GET",
          "summary": "[ESPI] Retrieve a Reading Type Using it's ID",
          "nickname": "getReadingType",
          "produces":
					[
            "application/xml"
          ],
          "authorizations":
					[
            "oauth2"
          ],
          "parameters":
